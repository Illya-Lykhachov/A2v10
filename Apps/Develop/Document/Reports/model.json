/*Document/reports*/
{
  "schema": "a2v10demo",
  "model": "Report",
  "actions": {
    "index": {
      "index": true,
      "view": "/Document/Reports/Index.view"
    },
    "show": {
      "indirect": true,
      "targetModel": {
        "model": "{{Report.Model}}",
        "view": "/document/Reports/{{Report.View}}",
        "schema": "{{Report.Schema}}",
        "template": "{{Report.Template}}"
      },
      "query": {
        "From": { "type": "Date" },
        "To": { "type": "Date" }
      }
    }
  }
}
