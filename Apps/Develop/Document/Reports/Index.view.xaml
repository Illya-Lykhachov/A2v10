<!-- Demo page -->
<Page xmlns="clr-namespace:A2v10.Xaml;assembly=A2v10.Xaml">
    <Repeater ItemsSource="{Bind Reports}">
        <Hyperlink Icon="Approve" Content="{Bind Name}" Padding="10" Block="True"
            Command="{BindCmd Open, 
            Url='/Document/Reports/Show', Argument={Bind Id}}" />
    </Repeater>
</Page>

    