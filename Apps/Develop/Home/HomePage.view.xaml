<!-- Home Page -->
<Page xmlns="clr-namespace:A2v10.Xaml;assembly=A2v10.Xaml"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Grid>
		<Toolbar>
			<Button Content="Test"></Button>
			<Button>
				<TextBox />
			</Button>
		</Toolbar>
		<DataGrid ItemsSource="{Bind Customers}">
			<DataGridColumn Header="Id" />
			<DataGridColumn Header="Name" />
			<DataGridColumn Header="Amount"/>
			<DataGridColumn Header="Complex">
				<TextBox></TextBox>
			</DataGridColumn>
		</DataGrid>
	</Grid>
</Page>

    