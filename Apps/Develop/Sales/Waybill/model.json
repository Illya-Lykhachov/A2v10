{
	"schema": "a2demo",
	"model": "Waybill",
	"actions": {
		"index": {
			"index": true,
			"view": "index.view",
			"template": "index.template"
		},
		"edit": {
			"model": "Document",
			"view": "waybill.view",
			"template": "waybill2.template",
			/*
			"parameters": {
				"Kind": "TEST"
			},
			*/
			"merge": {
				"schema": "a2demo",
				"model": "Document.Merge"
			}
		}
	},
	"dialogs": {
		"confirmPhone": {
			"model": "ConfirmPhone",
			"view": "confirm.dialog",
			"template": "confirm.template"
		},
		"euSign": {
			"view": "eusign.dialog",
			"model": "EUSign",
			"template": "eusign.template",
			"twoPhase": true
		},
		"checkEuSign": {
			"view": "checkeusign.dialog",
			"model": "EUSign",
			"template": "checkeusign.template",
			"twoPhase": true
		},
		"showLinks": {
			"model": "",
			"template": "links.template",
			"view": "links.dialog"
		},
		"testTime": {
			"model": "Test.Time"
		}
	},
	"commands": {
		"sendVerifyCode": {
			"type": "clr",
			"clrType": "clr-type:A2v10.Web.Mvc.Hooks.SendVerifyCodeSmsHandler;assembly=A2v10.Web.Mvc",
			"async": true
		},
		"confirmVerifyCode": {
			"type": "clr",
			"clrType": "clr-type:A2v10.Web.Mvc.Hooks.SetPhoneNumberHandler;assembly=A2v10.Web.Mvc",
			"async": true
		},
		"startWorkflow": {
			"type": "startProcess",
			"file": "Workflows/TestWorkflow_v1"
		},
		"resumeWorkflow": {
			"type": "resumeProcess"
		},
		"attachReport": {
			"type": "clr",
			"clrType": "clr-type:A2v10.Reports.Actions.AttachStiReport;assembly=A2v10.Reports",
			"async": true,
			"parameters": {
				"Report": "sales/waybill/waybill",
				"Model": "Document",
				"Schema": "a2demo"
			}
		},
		"attachReportXlsx": {
			"type": "clr",
			"clrType": "clr-type:A2v10.Reports.Actions.AttachXlsxReport;assembly=A2v10.Reports",
			"async": true,
			"parameters": {
				"Report": "sales/waybill/waybill",
				"Model": "Document",
				"Schema": "a2demo"
			}
		},
		"saveTime": {
			"type": "sql",
			"procedure": "SaveDocumentTime"
		},
		"serverScript": {
			"type": "script",
			"template": "server.template",
			"model": "Document",
			"command": "serverProcessAndSave"
		},
		"callApi": {
			"type": "callApi",
			"parameters": {
				"url": "((MyApi.url))/companyInfo/[[Code]]",
				"method": "get",
				"headers": {
					"Authorization": "bearer ((MyApi.apiKey))"
				}
			}
		}
	},
	"reports": {
		"waybill": {
			"report": "waybill",
			"name": "Заголовок отчета",
			"model": "Document"
		}
	},
	"attachments": {
		"default": {

		}
	}
