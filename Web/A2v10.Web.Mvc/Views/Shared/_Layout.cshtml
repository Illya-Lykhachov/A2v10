<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - A2v10</title>
	<link href="/css/site.min.css" rel="stylesheet"/>
</head>
<body>
	<div id="app">
		<div class="navbar navbar-inverse navbar-fixed-top">
			<div class="container">
				<i class="fa fa-user"></i>
				<div class="navbar-collapse collapse">
					@Html.Partial("_LoginPartial")
				</div>
			</div>
		</div>
		<div class="container body-content">
			@RenderBody()
			<hr />
		</div>
		<footer class="footer">
			<p>&copy; @DateTime.Now.Year  @A2v10.Web.Mvc.Configuration.AppInfo.MainAssembly.Version</p>
		</footer>
	</div>
	<script type="text/javascript" src="/scripts/vue.min.js?v=2.4.2"></script>
</body>
</html>
